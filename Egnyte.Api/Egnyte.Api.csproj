<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard1.1</TargetFramework>
    <Version>0.1.21</Version>
    <Authors>Michał Białecki</Authors>
    <Company>Egnyte Inc</Company>
    <Product>Egnyte .Net</Product>
    <Description>.NET Standard class library for accessing the Egnyte API.</Description>
    <Copyright>Copyright 2020</Copyright>
    <PackageTags>Egnyte .net api wrapper storage cloud</PackageTags>
    <RepositoryUrl>https://github.com/lroos/egnyte-dotnet</RepositoryUrl>
    <PackageProjectUrl>https://github.com/egnyte/egnyte-dotnet</PackageProjectUrl>
    <PackageReleaseNotes>Added Tasks API</PackageReleaseNotes>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="RateLimitMessageHandler.cs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
    <PackageReference Include="System.Net.Http" Version="4.3.4" />
  </ItemGroup>

</Project>
